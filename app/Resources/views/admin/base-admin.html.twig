<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}GotIt - Admin{% endblock %}</title>
    {% block stylesheets %}

        <!-- Admin -->
        <link rel="stylesheet" type="text/css" href="{{ asset('sb-admin/vendor/bootstrap/css/bootstrap.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('sb-admin/vendor/metisMenu/metisMenu.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('sb-admin/dist/css/sb-admin-2.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('sb-admin/vendor/morrisjs/morris.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('sb-admin/vendor/font-awesome/css/font-awesome.min.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ asset('sb-admin/vendor/datatables-responsive/dataTables.responsive.css') }}">
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
    <div id="wrapper">
    {% block nav %}<!-- Navigation -->
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">

            <ul class="nav navbar-top-links navbar-right">
                <!-- /.dropdown -->
                <li>
                    <a href="{{ path('logout') }}">Log Out</a>
                    <!-- /.dropdown-user -->
                </li>
                <!-- /.dropdown -->
            </ul>
            <!-- /.navbar-top-links -->

            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">
                        <li>
                            <a href="{{ path('admin_dashboard') }}"><i class="fa fa-dashboard fa-fw"></i> Dashboard</a>
                        </li>
                        <li>
                            <a href="{{ path('admin_users') }}"><i class="fa fa-user fa-fw"></i> Users</a>
                        </li>
                        <li>
                            <a href="{{ path('admin_reports') }}"><i class="fa fa-files-o fa-fw"></i> Reports</a>
                        </li>
                    </ul>
                </div>
                <!-- /.sidebar-collapse -->
            </div>
            <!-- /.navbar-static-side -->
        </nav>
    {% endblock %}
    {% block body %}{% endblock %}

    {% block javascripts %}

        <!-- Admin -->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
        <script src="{{ asset('js/constants.js') }}"></script>
        <script src="{{ asset('js/ajax.js') }}"></script>
        <script src="{{ asset('sb-admin/vendor/bootstrap/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('sb-admin/vendor/metisMenu/metisMenu.min.js') }}"></script>
        <script src="{{ asset('sb-admin/vendor/raphael/raphael.min.js') }}"></script>
        <script src="{{ asset('sb-admin/vendor/morrisjs/morris.min.js') }}"></script>
        <script src="{{ asset('sb-admin/data/morris-data.js') }}"></script>
        <script src="{{ asset('sb-admin/dist/js/sb-admin-2.js') }}"></script>
        <script src="{{ asset('sb-admin/vendor/datatables/js/jquery.dataTables.min.js') }}"></script>
        <script src="{{ asset('sb-admin/vendor/datatables-plugins/dataTables.bootstrap.min.js') }}"></script>
        <script src="{{ asset('sb-admin/vendor/datatables-responsive/dataTables.responsive.js') }}"></script>
        <script src="{{ asset('sb-admin/vendor/datatables-editable/jquery.dataTables.editable.js') }}"></script>
        <script src="{{ asset('sb-admin/vendor/datatables-editable/jquery.jeditable.mini.js') }}"></script>

    {% endblock %}
    </div>
</body>
</html>
